<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>RipperStoreReuploader</AssemblyName>
    <RootNamespace>RipperStoreReuploader</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|x64'">
    <OutputPath>bin\x64\Debug\</OutputPath>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|x64'">
    <OutputPath>bin\x64\Release\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="AssetsTools.NET">
      <HintPath>G:\UserFiles\source\repos\ReuploaderToolForFriends\Libs\AssetsTools.NET.dll</HintPath>
    </Reference>
    <Reference Include="Blake2Sharp">
      <HintPath>G:\UserFiles\source\repos\ReuploaderToolForFriends\Libs\Blake2Sharp.dll</HintPath>
    </Reference>
    <Reference Include="librsync.net">
      <HintPath>G:\UserFiles\source\repos\ReuploaderToolForFriends\Libs\librsync.net.dll</HintPath>
    </Reference>
    <Reference Include="LZ4">
      <HintPath>G:\UserFiles\source\repos\ReuploaderToolForFriends\Libs\LZ4.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Content Include="LICENSE\ASSETSTOOLS.NET_AND_ASSETSVIEW_CODE_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\ASYNCENUMERABLE_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\BLAKE2SHARP_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\CLASSDATA.TPK_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\DOTZLIB_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\LIBRSYNCNET_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\LZ4NET_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\LZMA-SDK_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\NEWTONSOFT.JSON_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LICENSE\ZLIB_LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="classdata.tpk">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
    <PackageReference Include="Microsoft.DotNet.UpgradeAssistant.Extensions.Default.Analyzers" Version="0.3.330701">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
</Project>